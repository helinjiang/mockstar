{
  "name": "mockstar-local-server",
  "version": "1.1.1",
  "description": "Local server for MockStar",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build && npm run build-ui",
    "build": "babel --presets env,stage-0 src/ --out-dir lib/ --source-maps",
    "watch-compile": "npm run build -- --watch",
    "watch": "npm run watch-compile",
    "dev-ui": "cd ./webui && npm run start",
    "build-ui": "cd ./webui && npm run build",
    "test-mocha": "node ./node_modules/mocha/bin/_mocha",
    "test": "npm run eslint && npm run test-cov",
    "eslint": "eslint src/",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha"
  },
  "keywords": [
    "mock",
    "server"
  ],
  "author": "helinjiang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockstarjs/mockstar.git"
  },
  "bugs": {
    "url": "https://github.com/mockstarjs/mockstar/issues"
  },
  "homepage": "https://github.com/mockstarjs/mockstar#readme",
  "engines": {
    "node": ">=5.6.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "log4js": "^2.8.0",
    "method-override": "^2.3.10",
    "mockstar": "^1.1.1",
    "request": "^2.81.0",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "mocha": "^5.2.0",
    "mockstar-client": "^1.1.1",
    "superagent": "^3.8.3"
  }
}
