{
  "name": "mockstar-generators",
  "version": "1.3.12",
  "description": "generators for mockstar",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc -d && node ./run-after-build.js",
    "test": "npm run test:cover",
    "test:unit": "mocha 'test/**/*.test.{ts,tsx}'",
    "test:cover": "nyc npm run test:unit",
    "lint": "eslint src test --ext .ts"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "mock",
    "server"
  ],
  "author": "helinjiang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockstarjs/mockstar.git"
  },
  "bugs": {
    "url": "https://github.com/mockstarjs/mockstar/issues"
  },
  "homepage": "https://github.com/mockstarjs/mockstar#readme",
  "dependencies": {
    "fs-extra": "^7.0.1",
    "shelljs": "^0.8.3",
    "yeoman-environment": "^2.3.4",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/expect": "^24.3.0",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^7.0.2",
    "@types/shelljs": "^0.8.8",
    "@types/yeoman-environment": "^2.10.1",
    "@types/yeoman-generator": "^3.1.4",
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "mocha": "^8.1.2",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "walk-sync": "^2.2.0"
  }
}
